{
  "name": "@eqian/axios-vue",
  "version": "0.0.3",
  "description": "工具库",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "umd": "./dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "clean:dist": "rimraf dist",
    "start:build": "esno script/build.ts",
    "build:docs": "typedoc ./src/index.ts --out ./play",
    "build:types": "npm run clean:dist && tsc -b ./tsconfig.types.json",
    "build": "npm run build:types && cross-env NODE_ENV=production rollup -c",
    "test": "jest",
    "pretest": "npm run build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,vue}\" --fix",
    "parser": "husky install",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-option.js",
    "version": "conventional-changelog -p custom-config -i CHANGELOG.md -s -n ./changelog-option.js && git add CHANGELOG.md",
    "post:version": "git push --tags",
    "test:coverage": "jest --coverage",
    "release": "npm run build&&standard-version",
    "release:alpha": "npm run build&&standard-version --prerelease alpha",
    "release:rc": "standard-version --prerelease rc",
    "release:major": "npm run release -- --release-as major",
    "release:minor": "npm run release -- --release-as minor",
    "release:patch": "npm run release -- --release-as patch",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "author": "cz",
  "license": "MIT",
  "keywords": [
    "工具库",
    "axios",
    "vue"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-eslint": "^10.1.0",
    "compare-func": "^2.0.0",
    "consola": "^3.2.3",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "conventional-changelog-custom-config": "^0.3.1",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "esno": "^4.0.0",
    "fs-extra": "^11.2.0",
    "husky": "^8.0.2",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "lint-staged": "12.4.0",
    "prettier": "2.6.2",
    "prettier-eslint": "15.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^4.9.4",
    "rollup-plugin-esbuild": "^6.1.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.5.4",
    "vitepress": "^1.0.0-alpha.29",
    "yarn": "^1.22.19"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "lint-staged": {
    "src/**/*.{ts,js,vue,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@eqian/utils-vue": "^2.5.2",
    "axios": "^1.7.2",
    "vue-demi": "^0.14.6"
  }
}
